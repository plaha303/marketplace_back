{
  "compilerOptions": {
    "target": "ESNext", 
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react-jsx",
    "noUncheckedIndexedAccess": true,
    "resolveJsonModule": true,
    "moduleResolution": "node", 
    "module": "ESNext", 
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
